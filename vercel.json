{
  "version": 2,
  "functions": {
    "api/chatbot.js": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "KV_REST_API_URL": "@nicoai-resources",
      "KV_REST_API_TOKEN": "@nicoai-resources"
    }
  },
  "routes": [
    {
      "src": "/api/chatbot",
      "dest": "/api/chatbot.js"
    }
  ]
}