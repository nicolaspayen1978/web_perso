{
  "version": 2,
  "functions": {
    "api/chatbot.js": {
      "maxDuration": 30
    },
    "api/init.js": {
      "maxDuration": 30
    },
    "api/resources.js": {
      "maxDuration": 30
    },
    "api/saveMessage.js": {
      "maxDuration": 10
    },
    "api/conversations.js": {
      "maxDuration": 30
    },
    "api/conversation/[visitorID]/index.js": {
      "maxDuration": 25
    },
    "api/debug-kv.js": {
      "maxDuration": 10
    },
    "api/clear-kv.js": {
      "maxDuration": 10
    },
    "api/gallery/run-update.js": {
      "maxDuration": 60
    },
    "api/gallery/load.js": {
      "maxDuration": 10
    },
    "api/gallery/public-load.js": { 
      "maxDuration": 10 
    },
    "api/gallery/save.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/chatbot", "dest": "/api/chatbot.js" },
    { "src": "/api/init", "dest": "/api/init.js" },
    { "src": "/api/resources", "dest": "/api/resources.js" },
    { "src": "/api/saveMessage", "dest": "/api/saveMessage.js" },
    { "src": "/api/conversations", "dest": "/api/conversations.js" },
    { "src": "/api/conversation/(.+)", "dest": "/api/conversation/$1/index.js" },
    { "src": "/api/debug-kv", "dest": "/api/debug-kv.js" },
    { "src": "/api/clear-kv", "dest": "/api/clear-kv.js" },
    { "src": "/api/gallery/run-update", "dest": "/api/gallery/run-update.js" },
    { "src": "/api/gallery/load", "dest": "/api/gallery/load.js" },
    { "src": "/api/gallery/save", "dest": "/api/gallery/save.js" },
    { "src": "/api/gallery/public-load", "dest": "/api/gallery/public-load.js" },
    { "handle": "filesystem" },
    { "src": "/articles/(.*)", "dest": "https://nicolaspayen1978.github.io/Articles/$1"}
  ]
}